if { $PARAM == "IFC" } {
   add_wave "-noupdate -label CLK" $DISCARD_STAT_PATH/CLK
   add_wave "-noupdate -label RESET" $DISCARD_STAT_PATH/RESET
   add_wave "-noupdate -hex -label STATUS" $DISCARD_STAT_PATH/STATUS

   add wave -divider "RX interface"
