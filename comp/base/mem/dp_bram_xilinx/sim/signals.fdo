proc blk_BRAM { } {

    global BRAM_PATH
    add_wave "-noupdate -hex -label PIPE_ENA" /$BRAM_PATH/PIPE_ENA
    add_wave "-noupdate -hex -label REA" /$BRAM_PATH/REA
    add_wave "-noupdate -hex -label WEA" /$BRAM_PATH/WEA
    add_wave "-noupdate -hex -label ADDRA" /$BRAM_PATH/ADDRA
    add_wave "-noupdate -hex -label DIA" /$BRAM_PATH/DIA
    add_wave "-noupdate -hex -label DOA_DV" /$BRAM_PATH/DOA_DV
    add_wave "-noupdate -hex -label DOA" /$BRAM_PATH/DOA
    add_wave "-noupdate -hex -label PIPE_ENB" /$BRAM_PATH/PIPE_ENB
    add_wave "-noupdate -hex -label REB" /$BRAM_PATH/REB
    add_wave "-noupdate -hex -label WEB" /$BRAM_PATH/WEB
    add_wave "-noupdate -hex -label ADDRB" /$BRAM_PATH/ADDRB
    add_wave "-noupdate -hex -label DIB" /$BRAM_PATH/DIB
    add_wave "-noupdate -hex -label DOB_DV" /$BRAM_PATH/DOB_DV
    add_wave "-noupdate -hex -label DOB" /$BRAM_PATH/DOB
}
