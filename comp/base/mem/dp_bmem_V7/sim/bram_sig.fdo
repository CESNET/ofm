# Paths
set TB_PATH     "/testbench"
set BRAM_PATH   "/testbench/uut"

# include signals
source      "signals.fdo"

exec make
view wave
delete wave *

add wave -noupdate -label RESET -color magenta $TB_PATH/reset
add wave -noupdate -label CLK -color magenta $TB_PATH/clk
blk_BRAM

restart -f
run 200ns

wave zoomrange 0 205ns
