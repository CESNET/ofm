#INIT for MFIFO2MEM
add_wave "-noupdate -uns -label INIT" /testbench/init
add wave -divider "INPUT"
add_wave "-noupdate -label WRITE" /testbench/wr
add_wave "-noupdate -uns -label BLOCK_ADDR" /testbench/blk_addr
