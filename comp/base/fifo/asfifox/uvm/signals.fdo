proc all {NAME PATH} {
    add wave -divider "$NAME"
    add_wave "-noupdate  -hex"       $PATH/*
}

proc wr {NAME PATH} {
    add wave -divider "$NAME"      
    add_wave "-noupdate  -hex"       $PATH/WR_*
}

proc rd {NAME PATH} {
    add wave -divider "$NAME"      
    add_wave "-noupdate  -hex"       $PATH/RD_*
}
