proc blk_squarer_ifc { BASE } {

   add_wave "-noupdate -label clk    -color yellow     " $BASE/CLK

   add_wave "-noupdate -unsigned -label data           " $BASE/DATA
   add_wave "-noupdate -unsigned -label result         " $BASE/RESULT

}
