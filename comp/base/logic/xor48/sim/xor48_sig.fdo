# Paths
set TB_PATH      "/testbench"
set XOR48_PATH   "/testbench/uut"

# include signals
source      "signals.fdo"

exec make
view wave
delete wave *

add wave -noupdate -label RESET -color magenta $TB_PATH/reset
add wave -noupdate -label CLK -color magenta $TB_PATH/clk
blk_XOR48

restart -f
run 500 ns

wave zoomrange 0 1000ns
