source "./signals.fdo"
exec make
view wave
delete wave *

set MAC_PATH "/testbench/DUT_U/VHDL_DUT_U/obuf_i"

add wave -divider "DUT"

rx        RX        /testbench/DUT_U/VHDL_DUT_U
tx        TX        /testbench/DUT_U/VHDL_DUT_U
mi        MI32      /testbench/DUT_U/VHDL_DUT_U
all       MAC       "$MAC_PATH"

restart -f
run -all
