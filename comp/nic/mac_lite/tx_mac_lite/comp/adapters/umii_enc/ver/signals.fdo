proc mii {NAME PATH} {
    add wave -divider "$NAME"
    add_wave "-group {$NAME} -noupdate  -hex"       $PATH/MII_*
}

proc rx {NAME PATH} {
    add wave -divider "$NAME"      
    add_wave "-group {$NAME} -noupdate  -hex"       $PATH/RX_*
}

proc int {NAME PATH} {
    add wave -divider "$NAME"      
    add_wave "-group {$NAME} -noupdate  -hex"       $PATH/s_*
}
