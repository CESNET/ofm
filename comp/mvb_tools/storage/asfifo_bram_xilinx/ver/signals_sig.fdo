source "./signals.fdo"
exec make
view wave
delete wave *

add wave -divider "FIFO"
add_wave "-noupdate -color yellow -label RESET"   /testbench/RESET

rx  RX  /testbench/DUT_U/VHDL_DUT_U
tx  TX  /testbench/DUT_U/VHDL_DUT_U

restart -f
run -all
