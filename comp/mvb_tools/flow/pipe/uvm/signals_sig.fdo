source "./signals.fdo"
view wave
delete wave *

add wave -divider "MVB_PIPE"

add_wave "-noupdate -color yellow -label RST"   /testbench/RST
add_wave "-noupdate -color yellow -label CLK"   /testbench/CLK

all ALL /testbench/DUT_U/VHDL_DUT_U

config wave -signalnamewidth 1
