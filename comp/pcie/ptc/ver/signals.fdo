   eval "add wave  -divider {}"
   add_wave " -noupdate -color #eecc66 -label up_mvb_asfifo_out -hex"   $PATH2/org_up_mvb_asfifo_out

   eval "add wave  -divider {}"
   add_wave " -noupdate -label up_mfb_trans_out -hex"   $PATH2/org_up_mfb_asfifo_out

   eval "add wave  -divider {}"
