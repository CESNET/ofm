set MFB_TRANSFORMER_PATH "/testbench/uut_i"

exec make
view wave
delete wave *

source "signals.fdo"

add wave -divider "Clock/Reset"
blk_MFB_TRANSFORMER_CR

add wave -divider "Input interface"
blk_MFB_TRANSFORMER_RX

add wave -divider "Output interface"
blk_MFB_TRANSFORMER_TX

restart -f
run 2 us
wave zoomrange 0 400ns
