proc rx {NAME PATH} {
    add wave -divider "$NAME"
     add_wave "-noupdate  -hex"       $PATH/RX_*
}

proc tx0 {NAME PATH} {
    add wave -divider "$NAME"      
     add_wave "-noupdate  -hex"       $PATH/TX0_*
}

proc tx1 {NAME PATH} {
    add wave -divider "$NAME"      
     add_wave "-noupdate  -hex"       $PATH/TX1_*
}

proc all {NAME PATH} {
    add wave -divider "$NAME"      
     add_wave "-noupdate  -hex"       $PATH/*
}
