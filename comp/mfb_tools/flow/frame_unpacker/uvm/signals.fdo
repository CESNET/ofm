proc mfb_rx {NAME PATH} {
    add_wave "-group {$NAME} -noupdate -hex" $PATH/RX_*
}

proc mfb_tx {NAME PATH} {
    add_wave "-group {$NAME} -noupdate -hex" $PATH/TX_*
}

proc all {NAME PATH} {
    add_wave "-group {$NAME} -noupdate -hex" $PATH/*
}
