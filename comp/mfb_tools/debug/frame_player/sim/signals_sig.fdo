# Paths
set UUT_PATH   "/testbench/uut"

source			"signals.fdo"

exec make
view wave
delete wave *

add_wave "-noupdate -color yellow -label RST" /testbench/RESET
add_wave "-noupdate -color yellow -label CLK" /testbench/CLK

mi32 "$UUT_PATH"
tx   "$UUT_PATH"
sig  "$UUT_PATH"


