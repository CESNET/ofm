# Paths
set TB_PATH          "/testbench"
set AXI2FLU_PATH   "/testbench/uut"

# include signals
source      "signals.fdo"

exec make
view wave
delete wave *

add wave -divider "COMMON INTERFACE"
add wave -noupdate -label RESET -color magenta $TB_PATH/RESET
add wave -noupdate -label CLK -color magenta $TB_PATH/CLK
blk_AXI2FLU_RX

blk_AXI2FLU_TX

